<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.njumzc.gitlabtss">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".features.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".features.teacher.TeacherMainActivity" />
        <activity android:name=".features.teacher.TeacherClassActivity" />
        <activity android:name=".features.teacher.TeacherStudentsActivity" />
        <activity android:name=".features.WorkListActivity" />
        <activity android:name=".features.teacher.TeacherWorkDetailActivity" />
        <activity android:name=".features.student.StudentMainActivity" />
        <activity android:name=".features.student.StudentAnalyseActivity"></activity>
    </application>

</manifest>